{
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "parser": "js",
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true,
        "bracketSpacing": true,
        "arrowFunctionParentheses": "always"
      }
    },
    {
      "files": "*.json",
      "options": {
        "parser": "json",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": false
      }
    }
  ]
}
